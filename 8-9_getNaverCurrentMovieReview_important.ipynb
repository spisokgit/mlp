    "# 현재상영중인 영화에 대한 리뷰를 모집하는 코딩=> 리뷰 데이터 부족함\n",
    "def getMovieUrl(movie_id):\n",
    "    url = \"https://movie.naver.com/movie/point/af/list.nhn?st=mcode&target=after&sword=%s\" % (movie_id)\n",
    "    return url\n",
    "\n",
    "def getData(url):\n",
    "    reviewdata_list = []\n",
    "    for page_num in range(1,50):\n",
    "        if (page_num > 51) : break # page_number 10ea \n",
    "        movie_url = url+\"&page=\"+str(page_num)\n",
    "        #print(movie_url)\n",
    "        soup = BeautifulSoup(requests.get(movie_url).text, 'lxml') # (re.sub(\"&#(?![0-9])\", \"\", data), \"html.parser\")\n",
    "        #rating_cnt 평점\n",
    "        rating_cnt = soup.select('#old_content > h5 > div > strong')[0].text\n",
    "        rs = soup.select('#old_content > table')[0].find('tbody').findAll('tr') # page마다 tr 10ea  # select(\".score_result li\")\n",
    "            \n",
    "        if not len(rs): # review 한개도 안 달린 경우 # rs==[] # len(rs)==0\n",
    "            reviewdata_dic={}\n",
    "            reviewdata_dic['review'] = ''\n",
    "            reviewdata_dic['rating'] = ''\n",
    "            reviewdata_dic['review_id'] = ''\n",
    "            reviewdata_list.append(reviewdata_dic)\n",
    "        else:    \n",
    "            try:\n",
    "                for r in rs: # rs list <======================================== important (because bs메서드 select() 리턴 list)\n",
    "                    reviewdata_dic={}\n",
    "                    rating = re.search('<em.*\\/em>+', str(r)).group(0)\n",
    "                    rating = re.search('[0-9]+', rating).group(0).strip()       \n",
    "                    review = re.search('<br\\/>.*', str(r)).group(0).replace('<br/>', '').strip() \n",
    "                    review_id = re.search('<td.*\\/td>+', str(r)).group(0)\n",
    "                    review_id = re.search('[0-9]+', review_id).group(0).strip()       \n",
    "                    if (review == '') : continue  # 공백 댓글 통과\n",
    "                    if (int(rating) > 8):  # positive 9 10\n",
    "                        reviewdata_dic['review'] = review\n",
    "                        reviewdata_dic['rating'] = rating\n",
    "                        reviewdata_dic['review_id'] = review_id\n",
    "                        reviewdata_dic['label'] = 1\n",
    "                    elif (int(rating) < 5): # negative 1 2 3 4\n",
    "                        reviewdata_dic['review'] = review\n",
    "                        reviewdata_dic['rating'] = rating\n",
    "                        reviewdata_dic['review_id'] = review_id\n",
    "                        reviewdata_dic['label'] = 0\n",
    "                    else:                    # neutral\n",
    "                        pass\n",
    "                    reviewdata_list.append(reviewdata_dic)\n",
    "\n",
    "            except (IndexError, AttributeError) as e:\n",
    "                print(e, \"파싱에서 에러입니다.\")\n",
    "                return None\n",
    "            except (AssertionError) as e:\n",
    "                print(e, 'Sleep for %s' % SLEEP)\n",
    "                time.sleep(SLEEP)\n",
    "            except Exception as e:\n",
    "                print(e, '음 여기까진 생각을 못했는데...')\n",
    "\n",
    "    #print(reviewdata_list)\n",
    "    return rating_cnt, reviewdata_list\n",
    "\n",
    "# main\n",
    "def getMovieReviewData(data):\n",
    "    data_t=[]\n",
    "    reviewdata=[]\n",
    "    review_cnt=0\n",
    "    for dic in data: # dic={'movie_id':value, 'movie_name':value}\n",
    "        url = getMovieUrl(dic['movie_id'])\n",
    "        #print(url)\n",
    "        rating_cnt, reviewdata_list = getData(url)\n",
    "        dic['rating_cnt'] = rating_cnt\n",
    "        review_cnt += len(reviewdata_list)\n",
    "        dic['review_data'] = reviewdata_list\n",
    "        #print(dic)\n",
    "        data_t.append(dic)\n",
    "        \n",
    "        # 별도로 reviewdata만 \n",
    "        reviewdata.append(reviewdata_list)\n",
    "    return data_t,review_cnt, reviewdata\n",
    "\n",
    "if __name__=='__main__':\n",
    "    URL = \"https://movie.naver.com/movie/point/af/list.nhn\"\n",
    "    data=getCurrentMovieIdName(URL)\n",
    "    print(f'현재 상영 중인 영화수는 {len(data)}개입니다.')\n",
