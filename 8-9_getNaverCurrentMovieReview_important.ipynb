    "url = \"https://movie.naver.com/movie/point/af/list.nhn\"\n",
    "# 네이버현재상영작 아이디 이름 가져오기\n",
    "def getCurrentMovieIdName(url):\n",
    "    try:\n",
    "        response = requests.get(url)\n",
    "        source = response.text.strip()\n",
    "        soup = BeautifulSoup(source, 'lxml') \n",
    "        rs = soup.select('#current_movie')[0].findAll('option')[1:] \n",
    "        #print(rs)\n",
    "    except: \n",
    "        print(\"해당사이트에 접근하거나 파싱할수가 없습니다.\")\n",
    "        \n",
    "    data=[]\n",
    "    if not len(rs): print(\"현재상영작이 없습니다.\")\n",
    "    for r in rs: \n",
    "        dic={}\n",
    "        id_ = re.search('[0-9]+', str(r)) #.replace('<br/>', '') # re.sub()\n",
    "        if (id_): \n",
    "            m_id = id_.group(0)\n",
    "        else :\n",
    "            print(\"영화 id 찾을 수 없습니다.\")\n",
    "        if m_id in data: continue # 동일한 아이디 있을 경우 통과\n",
    "        dic['movie_id'] = m_id\n",
    "        dic['movie_name'] = r.text  # fText(r.select('.star_score em'))  <= urllib\n",
    "        data.append(dic)\n",
    "    return data\n",
    "data=getCurrentMovieIdName(url)\n",
    "print(data[0])"
   ]
